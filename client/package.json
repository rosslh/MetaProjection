  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "getData": "node src/data/2019/federalScraper && node src/data/2019/byDistrictScraper",
    "build": "npm run getData && gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "serve": "gatsby serve",
    "cyTest": "cypress run",
    "cyTestGui": "cypress open",
    "runTestServer": "gatsby serve --port 3939",
    "test": "server-test runTestServer 3939 cyTest",
    "testGui": "server-test runTestServer 3939 cyTestGui",
    "clean": "rm -rf .cache && rm -rf public"
  },
  "repository": {
    "type": "git",
